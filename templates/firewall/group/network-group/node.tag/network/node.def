multi: 
type: ipv4net
help: Network-group member
val_help: ipv4net; IPv4 Subnet to match

syntax:expression: exec "sudo /opt/vyatta/sbin/vyatta-ipset.pl \
                           --action=check-member          \
                           --set-name=$VAR(../@)          \
                           --set-type=network             \
                           --member=\"$VAR(@)\"; "

syntax:expression: exec "                                 \
          /opt/vyatta/sbin/check_prefix_boundary $VAR(@)" \

